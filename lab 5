      "source": [
        "import numpy as np\n",
        "import matplotlib.pyplot as plt\n",
        "\n",
        "np.random.seed(42)\n",
        "N_days = 100\n",
        "customer_visits = np.random.randint(0, 11, size=N_days)\n",
        "\n",
        "pmf_values, pmf_counts = np.unique(customer_visits, return_counts=True)\n",
        "pmf_probabilities = pmf_counts / N_days\n",
        "\n",
        "cdf_probabilities = np.cumsum(pmf_probabilities)\n",
        "\n",
        "plt.figure(figsize=(12, 6))\n",
        "\n",
        "plt.subplot(1, 2, 1)\n",
        "plt.bar(pmf_values, pmf_probabilities, color='skyblue', edgecolor='black')\n",
        "plt.title('Probability Mass Function (PMF)')\n",
        "plt.xlabel('Number of Customers')\n",
        "plt.ylabel('Probability')\n",
        "\n",
        "plt.subplot(1, 2, 2)\n",
        "plt.step(pmf_values, cdf_probabilities, where='post', color='orange', linewidth=2)\n",
        "plt.title('Cumulative Distribution Function (CDF)')\n",
        "plt.xlabel('Number of Customers')\n",
        "plt.ylabel('Cumulative Probability')\n",
        "\n",
        "plt.tight_layout()\n",
        "plt.show()\n"
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "from scipy.stats import binom\n",
        "\n",
        "N_students = 100\n",
        "p_pass = 0.7\n",
        "\n",
        "prob_60_pass = binom.pmf(60, N_students, p_pass)\n",
        "print(f\"Probability that exactly 60 students pass: {prob_60_pass}\")\n",
        "\n",
        "prob_at_least_75_pass = 1 - binom.cdf(74, N_students, p_pass)\n",
        "print(f\"Probability that at least 75 students pass: {prob_at_least_75_pass}\")\n",
        "\n",
        "expected_pass = N_students * p_pass\n",
        "print(f\"Expected number of students passing: {expected_pass}\")\n"
      ],
      "metadata": {
